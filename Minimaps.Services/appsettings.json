{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Generator": "Information",
      "Minimaps.CLI.DatabaseMigrationService": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      },
      "IncludeScopes": true,
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  },
  "BackendUrl": "",
  "Blizztrack": {
    "CachePath": "",
    // CONSERVATIVE PROFILE (recommended for production)
    "RateLimitPermits": 300,
    "RateLimitWindowSeconds": 60,
    "RateLimitSegments": 12,
    "QueueLimit": 2147483647, // Set to int.MaxValue to allow unlimited queuing - requests will wait instead of being rejected
    "ConcurrencyLimit": 2,
    "ConcurrencyQueueLimit": 2147483647, // Set to int.MaxValue to allow unlimited queuing for concurrency limiter as well
    "MaxRetryAttempts": 3,
    "RetryBaseDelaySeconds": 1.0,
    "RetryMaxDelaySeconds": 30.0
  },
  "Services": {
    "UpdateMonitor": {
      "CachePath": "",
      "Products": [
        "wow",
        "wow_classic",
        "wowt",
      ]
    },
    "EventWebhook": ""
  }
}