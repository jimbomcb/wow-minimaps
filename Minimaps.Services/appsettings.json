{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Generator": "Information",
      "Minimaps.CLI.DatabaseMigrationService": "Information",
      "Polly": "Information",
      "System.Net.Http.HttpClient.Default": "Warning",
      "Npgsql.Command": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      },
      "IncludeScopes": true,
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  },
  "Blizztrack": {
    "CachePath": "",
    "RateLimitPermits": 1200,
    "RateLimitWindowSeconds": 60,
    "RateLimitSegments": 12,
    "QueueLimit": 2147483647, // int32 max
    "ConcurrencyLimit": 8,
    "ConcurrencyQueueLimit": 2147483647, // int32 max
    "MaxRetryAttempts": 3,
    "RetryBaseDelaySeconds": 1.0,
    "RetryMaxDelaySeconds": 30.0
  },
  "Services": {
    "ProductDiscovery": {
      "Products": [ "wow*" ],
      "ProductExclude": {
        "wowe": "BLTE exceptions parsing Build 9.0.1.35078, need to investigate"
      }
    },
    "ScanMaps": {
      "CompressionLevel": 100,
      "SpecificMaps": [],
      "CachePath": ""
    },
    "EventWebhook": ""
  },
  "LocalTileStore": {
    "Path": ""
  }
}