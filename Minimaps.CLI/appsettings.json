{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Generator": "Information",
      "Minimaps.CLI.DatabaseMigrationService": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      },
      "IncludeScopes": true,
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  },
  "Blizztrack": {
    "CachePath": "D:\\cache\\lfs",
    "RateLimitPermits": 600,
    "RateLimitWindowSeconds": 60,
    "RateLimitSegments": 12,
    "QueueLimit": 2147483647,
    "ConcurrencyLimit": 3,
    "ConcurrencyQueueLimit": 2147483647,
    "MaxRetryAttempts": 3,
    "RetryBaseDelaySeconds": 1.0,
    "RetryMaxDelaySeconds": 30.0
  },
  "LocalTileStore": {
    "Path": "D:\\cache\\tiles"
  }
}